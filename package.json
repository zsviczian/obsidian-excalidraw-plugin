    "mathjax-full": "^3.2.2",
    "monkey-around": "^2.3.0",
    "nanoid": "^4.0.2",
    "opentype.js": "^1.3.4",
    "polybooljs": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "roughjs": "^4.5.2",
    "woff2sfnt-sfnt2woff": "^1.0.0",
    "es6-promise-pool": "2.5.0"
